<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE MEEGO_INPUT_METHOD SYSTEM 'VirtualKeyboardToolbarDTD.dtd'>
<input-method version="1">
  <toolbar>
    <items>
      <button name="_Tab" text="t">
        <actions>
          <sendkeysequence keysequence="Tab"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Ctrl" text="c">
        <actions>
          <sendkeysequence keysequence="Ctrl+Shift+Alt+C"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Alt" text="a">
        <actions>
          <sendkeysequence keysequence="Ctrl+Shift+Alt+A"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Esc" text="e">
        <actions>
          <sendkeysequence keysequence="Esc"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Up" text="&#x2b06;">
        <actions>
          <sendkeysequence keysequence="Up"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Down" text="&#x2b07;">
        <actions>
          <sendkeysequence keysequence="Down"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Left" text="&#x2b05;">
        <actions>
          <sendkeysequence keysequence="Left"> </sendkeysequence>
        </actions>
      </button>
      <button name="_Right" text="&#x27a1;">
        <actions>
          <sendkeysequence keysequence="Right"> </sendkeysequence>
        </actions>
      </button>
    </items>
    <layout>
      <item name="_Tab" />
      <item name="_Ctrl" />
      <item name="_Alt" />
      <item name="_Esc" />
      <item name="_Up" />
      <item name="_Down" />
      <item name="_Left" />
      <item name="_Right" />
    </layout>
  </toolbar>
</input-method>
